    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/static/styles.css">
</head>

<body>

    <label for="mode-select">Choose a mode:</label>
    <select name="modes" id="mode-select">
        <option value="no">선택안함</option>
        <option value="total">전체</option>
        <option value="player">용병구함</option>
        <option value="match">매치구함</option>
    </select>

    <label for="date-select">date:</label>
    <select name="date" id="date-select">
        <option value="default">최신순</option>
        <option value="up">오름차순</option>
        <option value="down">내림차순</option>
    </select>
    <form id=submitForm action="">
        <input type="text" placeholder="Enter" id='searchContext'>
        <input type="submit" value="submit" id='searchBtn'>
    </form>
    <ul class="contents"></ul>


    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="/static/footma.js"></script>

    <ul class="content"></ul>
    <a href="http://localhost:3000/user/footma/paging">footma</a>


</body>


</html>